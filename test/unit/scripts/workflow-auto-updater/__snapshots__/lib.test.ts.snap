    runs-on: ubuntu-latest
    outputs:
      matrix: \${{ steps.create-matrix.outputs.result }}
    steps:
      - name: Collect Metrics
        id: collect-gha-metrics
        uses: smartcontractkit/push-gha-metrics-action
        with:
